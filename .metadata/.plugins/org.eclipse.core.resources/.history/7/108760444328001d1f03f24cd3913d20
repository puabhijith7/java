package aa;

public class Multi {

	public static void main(String[] args) {
		for(int i=1;i<11;i++)
		{
			System.out.println(i+" * 24 = "+(i*24));
		}
		for(int i=1;i<11;i++)
		{
			System.out.println(i+" * 50 = "+(i*50));
		}
		for(int i=1;i<11;i++)
		{
			System.out.println(i+" * 29 = "+(i*29));
		}

	}

}
