package aa;

public class Diamondfull {

	public static void main(String[] args) {
		for (int i=0;i<8;i++)
		{
			int c=i;
			while(c!=10)
			{
				System.out.print(" ");
				c++;
			}
			System.out.print("* ");	
			/*for (int j=i;j>1;j--)
			{
				
				System.out.print("10");
			}
			
			System.out.print("1\n");
		}*/
	}
		System.out.println();		

}

	}


