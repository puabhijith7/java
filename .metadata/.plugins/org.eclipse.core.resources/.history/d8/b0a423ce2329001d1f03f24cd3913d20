package oops;
class c
{
	int i;
	public void printNum()
	{
		System.out.println(i);
	}
}
class sub
{
	int j;
	public void printNum()
	{
		System.out.println(j);
	}
}

public class Inheritance8 {

	public static void main(String[] args) {
		sub obj=new sub();
		int i=10;
        obj.j=i;
        obj.printNum();
	}

}
